#-----------------------------------------------------
# .magicrc startup file for ETRI050 poject
#-----------------------------------------------------

path sys +../tech
#tech load SCN3ME_SUBM.30 -noprompt
tech load SCN3ME_SUBM.30.ETRI -noprompt

#--------------------------
# scalegrid a b scale magic units vs. lambda=30[angstrom]
scalegrid 1 2

set GND gnd
set VDD vdd

drc euclidean on
drc off

addpath ~/ETRI050_DesignKit/digital_ETRI
addpath ~/ETRI050_DesignKit/analog_ETRI
addpath ~/ETRI050_DesignKit/analog_ETRI/GDS_Magic
addpath ~/ETRI050_DesignKit/pads_ETRI050
addpath ~/ETRI050_DesignKit/pads_ETRI050/GDS_Magic

grid on
snap off

macro Control_XK_Pointer_Button4 {zoom 0.5}
macro Control_XK_Pointer_Button5 {zoom 2}

